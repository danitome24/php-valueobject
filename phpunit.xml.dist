<phpunit
    bootstrap="vendor/autoload.php"
    colors="true"
    syntaxCheck="false">

    <testsuites>
        <testsuite name="php-valueobject">
            <directory>tests</directory>
        </testsuite>
    </testsuites>

    <filter>
        <whitelist>
            <directory>src</directory>
            <exclude>
                <directory>tests</directory>
            </exclude>
        </whitelist>
    </filter>
</phpunit>
